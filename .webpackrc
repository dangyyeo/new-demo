{
    "theme": {
        "@primary-color": "#2eb5a4"
    },
    "proxy": {
        "/api": {
            "target": "http://jsonplaceholder.typicode.com/",
            "changeOrigin": true,
            "pathRewrite": { "^/api" : "" }
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ],
    "extraBabelPlugins": [
        ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true }]
    ],
    "env": {
        "development": {
            "extraBabelPlugins": ["dva-hmr"]
        }
    }
}
